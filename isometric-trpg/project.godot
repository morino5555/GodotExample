; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Project Starter"
run/main_scene="res://scenes/etc/title.tscn"
config/features=PackedStringArray("4.4")
config/icon="res://icon.png"

[autoload]

InputMapMgr="*res://utils/input_map/input_map_mgr.gd"
VolumeSettingsMgr="*res://utils/volume_settings_mgr.gd"
TransitionMgr="*res://utils/transition_mgr.tscn"
ScreenshotMgr="*res://utils/screenshot_mgr.tscn"
EventBus="*res://utils/event_bus.gd"

[debug]

gdscript/warnings/unused_signal=0
gdscript/warnings/unused_class_variable=true

[file_customization]

folder_colors={
"res://addons/": "yellow",
"res://assets/": "purple",
"res://scenes/": "green",
"res://tests/": "gray",
"res://utils/": "orange"
}

[global]

monitor=false
transition_mgr_default_speed=0.5
screenshot_mgr_max_images=1000
screenshot_mgr_resize_factor=1.0
screenshot_mgr_toggle_action_name="toggle_screenshots"
screenshot_mgr_frame_delay=0.1

